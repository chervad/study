ых вопросов по этому тегу на свой email.","success")}},"json")});$(document).on("click",".dropdown__menu .checkbox_force",function(event){event.stopPropagation()});$(document).on("click","[data-close-system-intrusive]",function(){var notice_block=$(this).parent();$.post("my/hide_system_intrusive",{id:$(this).data("close-system-intrusive")},function(resp){if(resp.success){notice_block.remove()}},"json")});$(document).on("click","[data-close-news]",function(){var notice_block=$(this).parent();$.post("my/hide_news",{id:$(this).data("close-news")},function(resp){if(resp.success){notice_block.remove()}},"json")});$(document).on("click","[data-close-utc]",function(){var notice_block=$(this).parent();$.post("my/utc_fm_close",{id:$(this).data("close-utc")},function(resp){if(resp.success){notice_block.remove()}},"json")});var registerBanner=$("#register-banner");if(registerBanner.length&&!sessionStorage["registerBannerClosed"]){setTimeout(function(){registerBanner.show().animate({bottom:0},700)},1e4)}$(document).on("click","[data-close-register-banner]",function(){safe_ga_event("banner","registration","close");sessionStorage["registerBannerClosed"]=true;var notice_block=$(this).parent();notice_block.remove()});if(typeof window.toster_flash_message!="undefined"){showFlashMessage(window.toster_flash_message[0],window.toster_flash_message[1])}if(window.location.hash.indexOf("#new_question")===0){var btn=$(".btn_add-question"),_t=window.location.hash.split(":");if(_t.length>1){btn.data("auth-ret",btn.data("auth-ret")+"?"+_t[1]);btn.attr("href",btn.attr("href")+"?"+_t[1])}$(".btn_add-question")[0].click()}});$.fn.setCursorPosition=function(position){if(this.length===0)return this;return $(this).setSelection(position,position)};$.fn.setSelection=function(selectionStart,selectionEnd){if(this.length===0)return this;input=this[0];if(input.createTextRange){var range=input.createTextRange();range.collapse(true);range.moveEnd("character",selectionEnd);range.moveStart("character",selecti